
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<script src="~/Scripts/jquery-3.7.0.js"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<br />
<button class="btn btn-primary" id="btn1">Button1</button>
<button class="btn btn-warning" id="btn2">Button2</button>
<button class="btn btn-danger" id="btn3">Button3</button>
<button class="btn btn-info" id="btn4">Button4</button>
<button class="btn btn-default" style="background-color:blueviolet" id="btn5">Button5</button>
<script>
    $('#btn1').click(function () {
        swal("Ürün Eklendi.");
    });
</script>

<script>
    $('#btn2').click(function () {
        swal("Burası başlık", "...Burası metin" );
    });
</script>

<script>
    $('#btn3').click(function () {
        swal({
            title: "Emin Misin?",
            text: "Bir kez silindiğinde bu dosyayı geri getiremezsiniz!",
            icon: "warning",
            buttons: true,
            dangerMode: true,
        })
            .then((willDelete) => {
                if (willDelete) {
                    swal("Ooops! Dosyanız silindi!", {
                        icon: "success",
                    });
                } else {
                    swal("Dosyanız güvende!");
                }
            });
    });
</script>

<script>
    $('#btn4').click(function () {
        swal({
            title: "Aferin!",
            text: "Düğmeye tıkladınız!",
            icon: "success",
            button: "Evet!",
        });
    });
</script>

<script>
    $('#btn5').click(function () {
        swal("Düğmeye veya modun dışına tıklayın.")
            .then((value) => {
                swal(`Döndürülen değer: ${value}`);
            });
    });
</script>